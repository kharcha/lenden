<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.registration"
    android:versionCode="1"
    android:versionName="1.0" xmlns:tools="http://schemas.android.com/tools">

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="18" tools:ignore="OldTargetApi"/>
<uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
    
        <activity
            android:name="com.example.registration.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:label="@string/app_name"
            android:name=".DashboardActivity"
             >
         <intent-filter >
                <action android:name="com.example.registration.DashboardActivity"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            </activity>
          
        <activity
            android:label="@string/app_name"
            android:name=".Home"
             >
         <intent-filter >
                <action android:name="com.example.registration.Home"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            </activity>
          
         <activity
            android:label="@string/app_name"
            android:name=".frndlist"
             >
         <intent-filter >
                <action android:name="com.example.registration.frndlist"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            </activity>
          
        
         <activity
            android:label="Register New Account"
            android:name=".RegisterActivity">
            
             <intent-filter >
                <action android:name="com.example.registration.RegisterActivity"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
      </activity>
    </application>

</manifest>
